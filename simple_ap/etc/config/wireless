config wifi-device  radio0
	option type     mac80211
	option channel  6
	option hwmode	11g
	option htmode	HT40
	option path     '10180000.wmac'
	option country  PL
	option noscan	1
	option disabled 1

config wifi-iface
	option device       radio0
	option network      lan
	option mode         ap
	option ssid         YourSSIDHere
	option key          'Your password here'
	option encryption   'psk2+tkip+ccmp'
	option macaddr      '02:2a:ac:62:de:68'
	option maxassoc	    4
