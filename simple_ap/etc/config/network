
config interface loopback
	option ifname lo
	option proto static
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals globals
        option ula_prefix 'fd27:70fa:5c1d::/48'

config interface lan
	list ifname eth0.1
	list ifname eth1
	list ifname usb0
	list ifname usb1
	option force_link 1
	option macaddr 'f6:b9:f1:da:0e:53'
	option type bridge
	option proto dhcp
	option igmp_snooping 0
	option stp 1

config interface lan6
	option proto dhcpv6
	option ifname @lan
	option reqprefix no

config alias 
	option interface lan
	option proto static
	option ipaddr '10.0.0.1'
	option netmask '255.255.255.0'

config switch
	option name switch0
	option reset 1
	option enable_vlan 1

config switch_vlan
	option device switch0
	option vlan 1
	option ports '0 6t'

config switch_port
        option device switch0
        option port 0
        option pvid 1
