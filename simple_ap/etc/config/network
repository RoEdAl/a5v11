
config interface loopback
	option ifname lo
	option proto static
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'
	option ipv6 0

config interface lan
	option ifname 'eth0.1 usb0'
	option force_link 1
	option macaddr 'f6:b9:f1:da:0e:53'
	option type bridge
	option proto dhcp
	option ipv6 0

config switch
	option name switch0
	option reset 1
	option enable_vlan 1

config switch_vlan
	option device switch0
	option vlan 1
	option ports '0 6t'
